# FOR GUNICORN USAGE
WORKER_TOTAL=4
THREADS=2
HOST=0.0.0.0
WORKER=gthread
TIMEOUT=300

# FOR WSGI & CONAINER & GUNICORN
EXPOSE_PORT=5000